context: '[N] -> { [] : N >= 0 }'
arrays:
- context: '[N] -> { [] : N >= 0 }'
  extent: '[N] -> { a[i0, i1] : i0 >= 0 and i1 >= 0 and i1 <= -1 + N }'
  element_type: int
statements:
- line: 8
  domain: '[N] -> { S_0[i, j] : i <= -1 + N and i >= 0 and j <= -1 + N and j >= i
    }'
  schedule: '[N] -> { S_0[i, j] -> [0, i, j] }'
  body:
    type: binary
    operation: =
    arguments:
    - type: access
      relation: '[N] -> { S_0[i, j] -> a[i, j] }'
      read: 0
      write: 1
    - type: access
      relation: '[N] -> { S_0[i, j] -> a[i, 0] : i <= 0; S_0[i, j] -> a[-1 + i, 0]
        : i >= 1 }'
      read: 1
      write: 0
